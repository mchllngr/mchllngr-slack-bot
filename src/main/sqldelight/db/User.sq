select:
SELECT *
FROM user
WHERE id = ?;

selectAll:
SELECT *
FROM user;

insert:
INSERT IGNORE INTO user(id)
VALUES (?);

updateBirthdate:
UPDATE user
SET birthdate = ?
WHERE id = ?;

updateIncludeBirthdateYear:
UPDATE user
SET includeBirthdateYear = ?
WHERE id = ?;

updateEnableBirthdateReminders:
UPDATE user
SET enableBirthdateReminders = ?
WHERE id = ?;

delete:
DELETE FROM user
WHERE id = ?;
